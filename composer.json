{
    "name": "vojtahavranek/menu-maker-api",
    "description": "API for Menu Maker application.",
    "type": "project",
    "require-dev": {
        "phpstan/phpstan-symfony": "^0.12.4",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MenuMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MenuMaker\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vojtech Havranek",
            "email": "vojtahavranek@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "nelmio/cors-bundle": "^2.0",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "5.0.*",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "5.0.*",
        "symfony/security-core": "5.0.*",
        "symfony/yaml": "5.0.*",
        "thecodingmachine/graphqlite-bundle": "^4.0",
        "ext-json": "*"
    }
}
