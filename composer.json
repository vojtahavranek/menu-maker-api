{
    "name": "vojtahavranek/menu-maker-api",
    "description": "API for Menu Maker application.",
    "type": "project",
    "require-dev": {
        "phpstan/phpstan": "^0.12.7",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MenuMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MenuMaker\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vojtech Havranek",
            "email": "vojtahavranek@gmail.com"
        }
    ],
    "require": {
        "thecodingmachine/graphqlite-bundle": "^4.0",
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    }
}
